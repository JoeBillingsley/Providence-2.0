<?xml version="1.0" encoding="UTF-8"?>

<?import custom_controls.ListToList?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="500.0" prefWidth="690.0" xmlns="http://javafx.com/javafx/8.0.40"
            fx:controller="ErrorMetricsSelectionController">
    <bottom>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <buttons>
                <Button fx:id="back" mnemonicParsing="false" text="Back" ButtonBar.buttonData="LEFT"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="3/3"/>
                <Button fx:id="next" disable="true" mnemonicParsing="false" text="Next"
                        ButtonBar.buttonData="NEXT_FORWARD"/>
            </buttons>
            <BorderPane.margin>
                <Insets left="10.0" right="10.0"/>
            </BorderPane.margin>
        </ButtonBar>
    </bottom>
    <center>
        <AnchorPane>
            <ListToList fx:id="errorMetrics"/>
        </AnchorPane>
    </center>
</BorderPane>
